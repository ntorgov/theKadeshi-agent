# TheKadeshi # Start #


<IfModule mod_suphp.c>
	suPHP_ConfigPath "C:\Users\Bagdad\Documents\web\project\antivir"
</IfModule>

<IfModule mod_php5.c>
	php_value auto_prepend_file "C:\Users\Bagdad\Documents\web\project\antivir/thekadeshi.php"
</IfModule>

<IfModule lsapi_module>
	php_value auto_prepend_file "C:\Users\Bagdad\Documents\web\project\antivir/thekadeshi.php"
</IfModule>

# 	LightSpeed
<IfModule LiteSpeed>
	php_value auto_prepend_file "C:\Users\Bagdad\Documents\web\project\antivir/thekadeshi.php"
</IfModule>

<Files ".user.ini">
	<IfModule mod_authz_core.c>
		Require all denied
	</IfModule>
	<IfModule !mod_authz_core.c>
		Order deny,allow
		Deny from all
	</IfModule>
</Files>

# 	Change 403 document
ErrorDocument 403 /thekadeshi.php?block

# 	Prevent mime based attacks
Header set X-Content-Type-Options "nosniff"

# 	Turn on IE8-IE9 XSS prevention tools
Header set X-XSS-Protection "1; mode=block"

# 	Remove server signature
ServerSignature Off

# 	Block xmlrpc
<Files xmlrpc.php>
	Order Allow,Deny
	deny from all
</Files>

# 	Blocked .htaccess file
<Files .htaccess>
	order allow,deny
	deny from all
</Files>

# TheKadeshi # End #
clude Off
php_value display_errors 1

php_value auto_prepend_file "/home/b/bagdad/thekadeshi/public_html/thekadeshi.php"
